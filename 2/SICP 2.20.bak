#lang racket/base


(define (same-parity x . y)

  (define parity (modulo x 2))
  
  (define (rec y)

    (cond ((null? y) '())
          ((= (modulo (car y) 2) parity) (cons (car y) (rec (cdr y))))
          (else (rec (cdr y))))
    
  )

  (append (list x) (rec y))
  )





(same-parity 1 2 3 4 5 6 7)
(same-parity 2 3 4 5 6 7)